<div class="jumbotron">
<h1>Login</h1>
<br />
<%= form_tag sessions_path do %>
Email: <%= text_field_tag :email, params[:email], class:"form-control" %><br />
Password: <%= password_field_tag :password, nil, class:"form-control"  %><br />
<%= submit_tag 'Log In', class:"btn btn-danger", style:"width:137px;float:right;" %>
<% end %>
<br />
</div>
